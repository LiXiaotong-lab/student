{"ast":null,"code":"export default {\n  // data：页面中需要的一些数据\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"),\n      courseData: [],\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      formVisible: false,\n      form: {},\n      name: null\n    };\n  },\n  // mounted：页面加载完毕后立即执行的函数\n  mounted() {\n    this.loadCourse(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    loadCourse(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get(\"/course/selectPage\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        if (res.code === \"200\") {\n          this.courseData = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleSelectionChange(rows) {\n      // 当前选中的所有的行数据\n      this.ids = rows.map(v => v.id);\n    },\n    choose(id) {\n      // 选课\n      this.$confirm(\"您确定选课吗？\", \"确认选课\", {\n        type: \"info\"\n      }).then(() => {\n        this.$request.post(\"/course/choose/\" + id).then(res => {\n          if (res.code === \"200\") {\n            // 表示操作成功\n            this.$message.success(\"选课成功\");\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    handleCurrentChange(pageNum) {\n      this.loadCourse(pageNum);\n    },\n    // 跳转函数\n    navTo(url) {\n      location.href = url;\n    },\n    reset() {\n      this.name = null;\n      this.loadCourse(1);\n    },\n    // 教师单个退选\n    del(id) {\n      this.$request.delete(\"/course/delete/\" + id).then(res => {\n        if (res.code === \"200\") {\n          this.$message.success(\"退选成功\");\n          this.loadCourse(1);\n        } else {\n          this.$message.error(\"此课程已有学生选择，无法退选！\");\n        }\n      });\n    },\n    delBatch() {\n      // 批量退选\n      if (!this.ids.length) {\n        this.$message.warning(\"请选择数据\");\n        return;\n      }\n      this.$confirm(\"您确定批量退选课程吗？\", \"确认退选\", {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete(\"/course/delete/batch\", {\n          data: this.ids\n        }).then(res => {\n          if (res.code === \"200\") {\n            // 表示操作成功\n            this.$message.success(\"操作成功\");\n            this.load(1);\n          } else {\n            this.$message.error(\"存在课程已被选择，无法删除！\"); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    }\n  }\n};","map":{"version":3,"names":["data","user","JSON","parse","localStorage","getItem","courseData","pageNum","pageSize","total","formVisible","form","name","mounted","loadCourse","methods","$request","get","params","then","res","code","list","$message","error","msg","handleSelectionChange","rows","ids","map","v","id","choose","$confirm","type","post","success","catch","handleCurrentChange","navTo","url","location","href","reset","del","delete","delBatch","length","warning","response","load"],"sources":["src/views/front/Course.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"main-content\">\n      <div\n        style=\"\n          width: 90%;\n          background-color: white;\n          margin: 30px auto;\n          border-radius: 20px;\n        \">\n        <div style=\"padding-bottom: 10px\">\n          <div\n            style=\"\n              display: flex;\n              font-size: 18px;\n              color: #000000ff;\n              line-height: 80px;\n              border-bottom: #cccccc 1px solid;\n            \">\n            <div style=\"flex: 3; margin-left: 20px\">课程信息</div>\n            <!-- 查找 -->\n            <div class=\"search\">\n              <el-input\n                placeholder=\"请输入课程名称查询\"\n                style=\"width: 200px\"\n                v-model=\"name\"></el-input>\n              <el-button\n                type=\"info\"\n                plain\n                style=\"margin-left: 10px\"\n                @click=\"loadCourse(1)\"\n                >查询</el-button\n              >\n              <el-button\n                type=\"warning\"\n                plain\n                style=\"margin-right: 50px\"\n                @click=\"reset\"\n                >重置</el-button\n              >\n            </div>\n          </div>\n        </div>\n        <div style=\"margin: 20px 0; padding: 0 50px\">\n          <div class=\"table\">\n            <el-table\n              :data=\"courseData\"\n              strip\n              @selection-change=\"handleSelectionChange\">\n              <!-- <el-table-column\n                prop=\"id\"\n                label=\"序号\"\n                width=\"70\"\n                align=\"center\"\n                sortable></el-table-column> -->\n              <el-table-column\n                prop=\"courseId\"\n                label=\"课程编号\"\n                width=\"100\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"name\"\n                label=\"课程名称\"\n                width=\"250\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"majorName\"\n                label=\"所属专业\"\n                width=\"200\"\n                align=\"center\"></el-table-column>\n\n              <el-table-column\n                prop=\"creditHours\"\n                label=\"学分\"\n                width=\"70\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"syllabus\"\n                label=\"教学大纲\"\n                width=\"500\"\n                show-overflow-tooltip\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"semester\"\n                label=\"学期\"\n                width=\"70\"\n      