{"ast":null,"code":"export default {\n  // data：页面中需要的一些数据\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"),\n      collegeData: [],\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      formVisible: false,\n      form: {}\n    };\n  },\n  // mounted：页面加载完毕后立即执行的函数\n  mounted() {\n    this.loadCollege(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    // 加载\n    loadCollege(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get(\"/college/selectPage\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        if (res.code === \"200\") {\n          this.collegeData = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    // 跳转函数\n    navTo(url) {\n      location.href = url;\n    },\n    // 分页\n    handleCurrentChange(pageNum) {\n      this.loadCollege(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["data","user","JSON","parse","localStorage","getItem","collegeData","pageNum","pageSize","total","formVisible","form","mounted","loadCollege","methods","$request","get","params","then","res","code","list","$message","error","msg","navTo","url","location","href","handleCurrentChange"],"sources":["src/views/front/College.vue"],"sourcesContent":["<template>\n  <div class=\"main-content\">\n    <div\n      style=\"\n        width: 90%;\n        background-color: white;\n        margin: 30px auto;\n        border-radius: 20px;\n      \">\n      <div style=\"padding-bottom: 10px\">\n        <div\n          style=\"\n            display: flex;\n            font-size: 18px;\n            color: #000000ff;\n            line-height: 80px;\n            border-bottom: #cccccc 1px solid;\n          \">\n          <div style=\"flex: 3; margin-left: 20px\">专业信息</div>\n        </div>\n    