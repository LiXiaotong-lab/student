{"ast":null,"code":"export default {\n  // data：页面中需要的一些数据\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"),\n      timetableData: [],\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      formVisible: false,\n      form: {},\n      rules: {\n        evaluate: [{\n          required: true,\n          message: \"请输入教师评价\",\n          trigger: \"blur\"\n        }],\n        score: [{\n          required: true,\n          message: \"请输入成绩\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n  // mounted：页面加载完毕后立即执行的函数\n  mounted() {\n    this.loadTimetable(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    // 编辑\n    editTimetable(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.formVisible = true;\n    },\n    // 保存按钮触发的逻辑\n    save() {\n      // 保存按钮触发的逻辑  它会触发新增或者更新\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.form.userId = this.user.id;\n          this.$request({\n            url: this.form.id ? \"/timetable/update\" : \"/timetable/add\",\n            method: this.form.id ? \"PUT\" : \"POST\",\n            data: this.form\n          }).then(res => {\n            if (res.code === \"200\") {\n              // 表示成功保存\n              this.$message.success(\"保存成功\");\n              this.loadTimetable(1);\n              this.formVisible = false;\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n    // 加载\n    loadTimetable(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get(\"/timetable/selectPage\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        if (res.code === \"200\") {\n          this.timetableData = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    // 分页\n    handleCurrentChange(pageNum) {\n      this.loadTimetable(pageNum);\n    },\n    // 跳转函数\n    navTo(url) {\n      location.href = url;\n    },\n    reset() {\n      this.name = null;\n      this.loadCollege(1);\n    },\n    // 删除\n    del(id) {\n      this.$request.delete(\"/timetable/delete/\" + id).then(res => {\n        if (res.code === \"200\") {\n          this.$message.success(\"退选成功\");\n          this.loadTimetable(1);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","user","JSON","parse","localStorage","getItem","timetableData","pageNum","pageSize","total","formVisible","form","rules","evaluate","required","message","trigger","score","mounted","loadTimetable","methods","editTimetable","row","stringify","save","$refs","formRef","validate","valid","userId","id","$request","url","method","then","res","code","$message","success","error","msg","get","params","list","handleCurrentChange","navTo","location","href","reset","name","loadCollege","del","delete"],"sources":["src/views/front/Timetable.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"main-content\">\n      <div\n        style=\"\n          width: 90%;\n          background-color: white;\n          margin: 30px auto;\n          border-radius: 20px;\n        \">\n        <div style=\"padding-bottom: 10px\">\n          <div\n            style=\"\n              display: flex;\n              font-size: 18px;\n              color: #000000ff;\n              line-height: 80px;\n              border-bottom: #cccccc 1px solid;\n            \">\n            <div style=\"flex: 3; margin-left: 20px\">选课记录</div>\n            <!-- 查找 -->\n            <div class=\"search\">\n              <el-select\n                v-model=\"searchType\"\n                placeholder=\"选择查询类型\"\n                style=\"width: 150px\">\n                <el-option label=\"教师编号\" value=\"teacherId\"></el-option>\n                <el-option label=\"课程编号\" value=\"courseId\"></el-option>\n                <el-option label=\"学生学号\" value=\"studentId\"></el-option>\n              </el-select>\n\n              <el-input\n                :placeholder=\"`请输入${\n                  searchType === 'teacherId'\n                    ? '教师编号'\n                    : searchType === 'courseId'\n                    ? '课程编号'\n                    : '学生学号' === 'studentId'\n                    ? '学生学号'\n                    : ''\n                }查询`\"\n                style=\"width: 200px; margin-left: 10px\"\n                v-model=\"searchValue\"></el-input>\n\n              <el-button\n                type=\"info\"\n                plain\n                style=\"margin-left: 10px\"\n                @click=\"load(1)\"\n                >查询</el-button\n              >\n              <el-button\n                type=\"warning\"\n                plain\n                style=\"margin-left: 10px\"\n                @click=\"reset\"\n                >重置</el-button\n              >\n            </div>\n          </div>\n        </div>\n        <div style=\"margin: 20px 0; padding: 0 50px\">\n          <div class=\"table\">\n            <el-table\n              :data=\"timetableData\"\n              strip\n              @selection-change=\"handleSelectionChange\">\n              <el-table-column\n                prop=\"id\"\n                label=\"序号\"\n                width=\"70\"\n                align=\"center\"\n                sortable></el-table-column>\n              <el-table-column\n                prop=\"studentId\"\n                label=\"学生学号\"\n                width=\"160\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"studentName\"\n                label=\"学生姓名\"\n                width=\"150\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"courseId\"\n                label=\"课程编号\"\n                width=\"150\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"courseName\"\n                label=\"课程名称\"\n                width=\"220\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"teacherId\"\n                label=\"教师编号\"\n                width=\"150\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"teacherName\"\n                label=\"教师姓名\"\n                width=\"180\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"time\"\n                label=\"上课时间\"\n                width=\"180\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"room\"\n                label=\"教室\"\n                show-overflow-tooltip\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"testtime\"\n                label=\"考试时间\"\n                width=\"180\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"testroom\"\n                label=\"考试教室\"\n                show-overflow-tooltip\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"score\"\n                label=\"成绩\"\n                width=\"150\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"evaluate\"\n                label=\"教师评价\"\n                width=\"150\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                label=\"操作\"\n                align=\"center\"\n                width=\"180\"\n                v-if=\"user.role === 'STUDENT' || user.role === 'TEACHER'\">\n                <template v-slot=\"scope\">\n                  <el-button\n                    size=\"mini\"\n                    type=\"primary\"\n                    plain\n                    @click=\"editTimetable(scope.row)\"\n                    >编辑</el-button\n                  >\n                  <el-button\n                    size=\"mini\"\n                    type=\"danger\"\n                    plain\n                    @click=\"del(scope.row.id)\"\n                    v-if=\"user.role === 'STUDENT'\"\n                    >退选</el-button\n                  >\n                </template>\n              </el-table-column>\n            </el-table>\n\n            <div class=\"pagination\" style=\"margin-top: 20px\">\n              <el-pagination\n                background\n                @current-change=\"handleCurrentChange\"\n                :current-page=\"pageNum\"\n                :page-sizes=\"[5, 10, 20]\"\n                :page-size=\"pageSize\"\n                layout=\"total, prev, pager, next\"\n                :total=\"total\">\n              </el-pagination>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <el-dialog\n      title=\"评价信息\"\n      :visible.sync=\"formVisible\"\n      width=\"40%\"\n      :close-on-click-modal=\"false\"\n      destroy-on-close>\n      <el-form\n        label-width=\"100px\"\n        style=\"padding-right: 50px\"\n        :model=\"form\"\n        :rules=\"rules\"\n        ref=\"formRef\">\n        <el-form-item label=\"成绩\" prop=\"score\" v-if=\"user.role === 'TEACHER'\">\n          <el-input v-model=\"form.score\" placeholder=\"成绩\"></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"教师评价\"\n          prop=\"evaluate\"\n          v-if=\"user.role === 'STUDENT'\">\n          <el-input v-model=\"form.evaluate\" placeholder=\"教师评价\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"formVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"save\"> 确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  // data：页面中需要的一些数据\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"),\n      timetableData: [],\n      pageNum: 1, // 当前的页码\n      pageSize: 10, // 每页显示的个数\n      total: 0,\n      formVisible: false,\n      form: {},\n      rules: {\n        evaluate: [\n          { required: true, message: \"请输入教师评价\", trigger: \"blur\" },\n        ],\n        score: [{ required: true, message: \"请输入成绩\", trigger: \"blur\" }],\n      },\n    };\n  },\n\n  // mounted：页面加载完毕后立即执行的函数\n  mounted() {\n    this.loadTimetable(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    // 编辑\n    editTimetable(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.formVisible = true;\n    },\n\n    // 保存按钮触发的逻辑\n    save() {\n      // 保存按钮触发的逻辑  它会触发新增或者更新\n      this.$refs.formRef.validate((valid) => {\n        if (valid) {\n          this.form.userId = this.user.id;\n          this.$request({\n            url: this.form.id ? \"/timetable/update\" : \"/timetable/add\",\n            method: this.form.id ? \"PUT\" : \"POST\",\n            data: this.form,\n          }).then((res) => {\n            if (res.code === \"200\") {\n              // 表示成功保存\n              this.$message.success(\"保存成功\");\n              this.loadTimetable(1);\n              this.formVisible = false;\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n\n    // 加载\n    loadTimetable(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request\n        .get(\"/timetable/selectPage\", {\n          params: {\n            pageNum: this.pageNum,\n            pageSize: this.pageSize,\n          },\n        })\n        .then((res) => {\n          if (res.code === \"200\") {\n            this.timetableData = res.data?.list;\n            this.total = res.data?.total;\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n    },\n // 分页\n    handleCurrentChange(pageNum) {\n      this.loadTimetable(pageNum);\n    },\n    // 跳转函数\n    navTo(url) {\n      location.href = url;\n    },\n      reset() {\n      this.name = null;\n      this.loadCollege(1);\n    },\n\n    // 删除\n    del(id) {\n      this.$request.delete(\"/timetable/delete/\" + id).then((res) => {\n        if (res.code === \"200\") {\n          this.$message.success(\"退选成功\");\n          this.loadTimetable(1);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n\n   \n  },\n};\n</script>\n"],"mappings":"AA4MA;EACA;EACAA,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,aAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA;MACAC,KAAA;QACAC,QAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAC,KAAA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;IACA;EACA;EAEA;EACAE,QAAA;IACA,KAAAC,aAAA;EACA;EACA;EACAC,OAAA;IACA;IACAC,cAAAC,GAAA;MACA,KAAAX,IAAA,GAAAT,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAqB,SAAA,CAAAD,GAAA;MACA,KAAAZ,WAAA;IACA;IAEA;IACAc,KAAA;MACA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAjB,IAAA,CAAAkB,MAAA,QAAA5B,IAAA,CAAA6B,EAAA;UACA,KAAAC,QAAA;YACAC,GAAA,OAAArB,IAAA,CAAAmB,EAAA;YACAG,MAAA,OAAAtB,IAAA,CAAAmB,EAAA;YACA9B,IAAA,OAAAW;UACA,GAAAuB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cACA;cACA,KAAAC,QAAA,CAAAC,OAAA;cACA,KAAAnB,aAAA;cACA,KAAAT,WAAA;YACA;cACA,KAAA2B,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;YACA;UACA;QACA;MACA;IACA;IAEA;IACArB,cAAAZ,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAwB,QAAA,CACAU,GAAA;QACAC,MAAA;UACAnC,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GACA0B,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAA9B,aAAA,GAAA6B,GAAA,CAAAnC,IAAA,EAAA2C,IAAA;UACA,KAAAlC,KAAA,GAAA0B,GAAA,CAAAnC,IAAA,EAAAS,KAAA;QA