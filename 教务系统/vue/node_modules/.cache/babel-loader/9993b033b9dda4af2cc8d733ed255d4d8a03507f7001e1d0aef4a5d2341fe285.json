{"ast":null,"code":"export default {\n  // data：页面中需要的一些数据\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"),\n      courseData: [],\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      formVisible: false,\n      form: {},\n      rules: {\n        evaluate: [{\n          required: true,\n          message: \"请输入教师评价\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n  // mounted：页面加载完毕后立即执行的函数\n  mounted() {\n    this.loadCourse(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    // 编辑\n    editCourse(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.formVisible = true;\n    },\n    // 保存按钮触发的逻辑\n    save() {\n      // 保存按钮触发的逻辑  它会触发新增或者更新\n      this.$refs.formRef.validate(valid => {\n        if (valid) {\n          this.form.userId = this.user.id;\n          this.$request({\n            url: this.form.id ? \"/course/update\" : \"/course/add\",\n            method: this.form.id ? \"PUT\" : \"POST\",\n            data: this.form\n          }).then(res => {\n            if (res.code === \"200\") {\n              // 表示成功保存\n              this.$message.success(\"保存成功\");\n              this.loadCourse(1);\n              this.formVisible = false;\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n    // 加载\n    loadCourse(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get(\"/course/selectPage\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        if (res.code === \"200\") {\n          this.courseData = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    // 跳转函数\n    navTo(url) {\n      location.href = url;\n    },\n    // 删除\n    del(id) {\n      this.$request.delete(\"/course/delete/\" + id).then(res => {\n        if (res.code === \"200\") {\n          this.$message.success(\"退选成功\");\n          this.loadCourse(1);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    // 分页\n    handleCurrentChange(pageNum) {\n      this.loadCourse(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["data","user","JSON","parse","localStorage","getItem","courseData","pageNum","pageSize","total","formVisible","form","rules","evaluate","required","message","trigger","mounted","loadCourse","methods","editCourse","row","stringify","save","$refs","formRef","validate","valid","userId","id","$request","url","method","then","res","code","$message","success","error","msg","get","params","list","navTo","location","href","del","delete","handleCurrentChange"],"sources":["src/views/front/Course.vue"],"sourcesContent":["<template>\n  <div class=\"main-content\">\n    <div\n      style=\"\n        width: 90%;\n        background-color: white;\n        margin: 30px auto;\n        border-radius: 20px;\n      \">\n      <div style=\"padding-bottom: 10px\">\n        <div\n          style=\"\n            display: flex;\n            font-size: 18px;\n            color: #000000ff;\n            line-height: 80px;\n            border-bottom: #cccccc 1px solid;\n          \">\n          <div style=\"flex: 3; margin-left: 20px\">选课记录</div>\n        </div>\n        <div style=\"margin: 20px 0; padding: 0 50px\">\n          <div class=\"table\">\n            <el-table :data=\"courseData\" strip>\n            \n              <el-table-column\n                prop=\"courseId\"\n                label=\"课程编号\"\n                width=\"100\"\n                align=\"center\"></el-table-column>\n              <el-table-column\n                prop=\"name\"\n                label=\"课程名称\"\n                width=\"\"\n                align=\"center\"></el-table-column>\n  <el-table-column\n          prop=\"majorName\"\n          label=\"所属专业\"\n          width=\"170\"\n          align=\"center\"></el-table-column>\n\n        <el-table-column\n          prop=\"creditHours\"\n          label=\"学分\"\n          width=\"70\"\n          align=\"center\"></el-table-column>\n        <el-table-column\n          prop=\"syllabus\"\n          label=\"教学大纲\"\n          width=\"500\"\n          show-overflow-tooltip\n          align=\"center\"></el-table-column>\n        <el-table-column\n          prop=\"semester\"\n          label=\"学期\"\n          width=\"70\"\n          align=\"center\"></el-table-column>\n\n\n\n\n\n\n\n\n\n\n\n\n            \n              <el-table-column\n                label=\"操作\"\n                align=\"center\"\n                width=\"180\"\n                v-if=\"user.role === 'STUDENT'\">\n                <template v-slot=\"scope\">\n                  <el-button\n                    size=\"mini\"\n                    type=\"primary\"\n                    plain\n                    @click=\"editCourse(scope.row)\"\n                    >编辑</el-button\n                  >\n                  <el-button\n                    size=\"mini\"\n                    type=\"danger\"\n                    plain\n                    @click=\"del(scope.row.id)\"\n                    >退选</el-button\n                  >\n                </template>\n              </el-table-column>\n            </el-table>\n\n            <div class=\"pagination\" style=\"margin-top: 20px\">\n              <el-pagination\n                background\n                @current-change=\"handleCurrentChange\"\n                :current-page=\"pageNum\"\n                :page-sizes=\"[5, 10, 20]\"\n                :page-size=\"pageSize\"\n                layout=\"total, prev, pager, next\"\n                :total=\"total\">\n              </el-pagination>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <el-dialog\n      title=\"评价信息\"\n      :visible.sync=\"formVisible\"\n      width=\"40%\"\n      :close-on-click-modal=\"false\"\n      destroy-on-close>\n      <el-form\n        label-width=\"100px\"\n        style=\"padding-right: 50px\"\n        :model=\"form\"\n        :rules=\"rules\"\n        ref=\"formRef\">\n        <el-form-item label=\"教师评价\" prop=\"evaluate\">\n          <el-input v-model=\"form.evaluate\" placeholder=\"教师评价\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"formVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"save\"> 确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  // data：页面中需要的一些数据\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"),\n      courseData: [],\n      pageNum: 1, // 当前的页码\n      pageSize: 10, // 每页显示的个数\n      total: 0,\n      formVisible: false,\n      form: {},\n      rules: {\n        evaluate: [\n          { required: true, message: \"请输入教师评价\", trigger: \"blur\" },\n        ],\n      },\n    };\n  },\n\n  // mounted：页面加载完毕后立即执行的函数\n  mounted() {\n    this.loadCourse(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    // 编辑\n    editCourse(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.formVisible = true;\n    },\n\n    // 保存按钮触发的逻辑\n    save() {\n      // 保存按钮触发的逻辑  它会触发新增或者更新\n      this.$refs.formRef.validate((valid) => {\n        if (valid) {\n          this.form.userId = this.user.id;\n          this.$request({\n            url: this.form.id ? \"/course/update\" : \"/course/add\",\n            method: this.form.id ? \"PUT\" : \"POST\",\n            data: this.form,\n          }).then((res) => {\n            if (res.code === \"200\") {\n              // 表示成功保存\n              this.$message.success(\"保存成功\");\n              this.loadCourse(1);\n              this.formVisible = false;\n            } else {\n              this.$message.error(res.msg); // 弹出错误的信息\n            }\n          });\n        }\n      });\n    },\n\n    // 加载\n    loadCourse(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request\n        .get(\"/course/selectPage\", {\n          params: {\n            pageNum: this.pageNum,\n            pageSize: this.pageSize,\n          },\n        })\n        .then((res) => {\n          if (res.code === \"200\") {\n            this.courseData = res.data?.list;\n            this.total = res.data?.total;\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n    },\n\n    // 跳转函数\n    navTo(url) {\n      location.href = url;\n    },\n\n    // 删除\n    del(id) {\n      this.$request.delete(\"/course/delete/\" + id).then((res) => {\n        if (res.code === \"200\") {\n          this.$message.success(\"退选成功\");\n          this.loadCourse(1);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n\n    // 分页\n    handleCurrentChange(pageNum) {\n      this.loadCourse(pageNum);\n    },\n  },\n};\n</script>\n"],"mappings":"AAqIA;EACA;EACAA,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,UAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA;MACAC,KAAA;QACAC,QAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EAEA;EACAC,QAAA;IACA,KAAAC,UAAA;EACA;EACA;EACAC,OAAA;IACA;IACAC,WAAAC,GAAA;MACA,KAAAV,IAAA,GAAAT,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAoB,SAAA,CAAAD,GAAA;MACA,KAAAX,WAAA;IACA;IAEA;IACAa,KAAA;MACA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAhB,IAAA,CAAAiB,MAAA,QAAA3B,IAAA,CAAA4B,EAAA;UACA,KAAAC,QAAA;YACAC,GAAA,OAAApB,IAAA,CAAAkB,EAAA;YACAG,MAAA,OAAArB,IAAA,CAAAkB,EAAA;YACA7B,IAAA,OAAAW;UACA,GAAAsB,IAAA,CAAAC,GAAA;YACA,IAAAA,GAAA,CAAAC,IAAA;cACA;cACA,KAAAC,QAAA,CAAAC,OAAA;cACA,KAAAnB,UAAA;cACA,KAAAR,WAAA;YACA;cACA,KAAA0B,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;YACA;UACA;QACA;MACA;IACA;IAEA;IACArB,WAAAX,OAAA;MACA,IAAAA,OAAA,OAAAA