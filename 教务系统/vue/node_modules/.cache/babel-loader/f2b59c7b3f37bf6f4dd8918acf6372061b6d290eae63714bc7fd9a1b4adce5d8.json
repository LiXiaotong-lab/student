ser: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"),\n      collegeData: [],\n      pageNum: 1, // 当前的页码\n      pageSize: 10, // 每页显示的个数\n      total: 0,\n      formVisible: false,\n      form: {},\n      name: \"\", // 搜索的学院名称\n    };\n  },\n\n  mounted() {\n    this.loadCollege(1);\n  },\n\n  methods: {\n    loadCollege(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request\n        .get(\"/college/selectPage\", {\n          params: {\n            pageNum: this.pageNum,\n            pageSize: this.pageSize,\n            name: this.name, // 加入搜索条件\n          },\n        })\n        .then((res) => {\n          if (res.code === \"200\") {\n            this.collegeData = res.data?.list;\n            this.total = res.data?.total;\n          } else {\n            this.$message.error(res.msg);\n          }\n        });\n    },\n\n    handleCurrentChange(pageNum) {\n      this.loadCollege(pageNum);\n    },\n\n    reset() {\n      this.name = \"\";\n      this.loadCollege(1);\n    },\n  },\n};\n</script>\n"],"mappings":"AAwFA;EACAA,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,WAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA;MACAC,IAAA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,WAAA;EACA;EAEAC,OAAA;IACAD,YAAAP,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAS,QAAA,CACAC,GAAA;QACAC,MAAA;UACAX,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAI,IAAA,OAAAA,IAAA;QACA;MACA,GACAO,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAf,WAAA,GAAAc,GAAA,CAAApB,IAAA,EAAAsB,IAAA;UACA,KAAAb,KAAA,GAAAW,GAAA,CAAApB,IAAA,EAAAS,KAAA;QACA;UACA,KAAAc,QAAA,CAAAC,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;IACA;IAEAC,oBAAAnB,OAAA;MACA,KAAAO,WAAA,CAAAP,OAAA;IACA;IAEAoB,MAAA;MACA,KAAAf,IAAA;MACA,KAAAE,WAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}